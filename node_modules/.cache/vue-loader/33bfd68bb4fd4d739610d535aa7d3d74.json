\n        type=\"password\"\r\n        left-icon=\"closed-eye\"\r\n        placeholder=\"请输入密码\"\r\n      >\r\n      </van-field>\r\n\r\n      <!-- 登录按键 -->\r\n      <van-button\r\n        color=\"#D41C1C\"\r\n        size=\"large\"\r\n        style=\"width: 70%\"\r\n        @click=\"submitLogin\"\r\n      >\r\n        登录\r\n      </van-button>\r\n      <!-- 忘记密码 -->\r\n      <van-row type=\"flex\" justify=\"center\" style=\"margin: 0.4rem 0 0.4rem 0\">\r\n        <van-col @click=\"toForgetPassword\">忘记密码</van-col>\r\n      </van-row>\r\n    </div>\r\n    <!-- 底部其它登录方式 -->\r\n    <div class=\"login-foot\">\r\n      <van-row>\r\n        <van-divider> 其它方式登录 </van-divider>\r\n      </van-row>\r\n      <div class=\"images\">\r\n        <van-image\r\n          width=\"1rem\"\r\n          height=\"1rem\"\r\n          src=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/login/wechat%402x.png\"\r\n        />\r\n        <van-image\r\n          width=\"1rem\"\r\n          height=\"1rem\"\r\n          src=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/login/tencent%402x.png\"\r\n        />\r\n        <van-image\r\n          width=\"1rem\"\r\n          height=\"1rem\"\r\n          src=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/login/sina%402x.png\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport cookie from \"js-cookie\";\r\nimport loginApi from \"../../../api/login\";\r\nimport { mapMutations } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {\r\n        password: \"\",\r\n        phone: null,\r\n      },\r\n      loginInfo: null,\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapMutations(\"token\", { changeLogin: \"changeLogin\" }),\r\n    toForgetPassword() {\r\n      this.$router.push(\"/forgetpassword\");\r\n    },\r\n    toHome() {\r\n      this.$router.replace(\"/home\");\r\n    },\r\n    submitLogin() {\r\n      loginApi.submitLogin(this.user).then((response) => {\r\n        if (response.data.success) {\r\n          this.userToken = response.data.data.token;\r\n          // 将用户token保存到vuex中\r\n          this.changeLogin({ Authorization: this.userToken });\r\n          //把token存在cookie中、也可以放在localStorage中\r\n          cookie.set(\"huiju_token\", response.data.data.token, {\r\n            domain: \"localhost\",\r\n          });\r\n          //登录成功根据token获取用户信息;\r\n          //TODO 为什么请求不到数据\r\n          loginApi.getLoginInfo().then((res) => {\r\n            console.log(res);\r\n            this.loginInfo = response.data;\r\n          });\r\n          this.axios\r\n            .get(\"http://172.18.1.101:8104/usermanage/user/auth/getLoginInfo\", {\r\n              headers: {\r\n                token: this.userToken,\r\n              },\r\n            })\r\n            .then((res) => {\r\n              console.log(res);\r\n              this.loginInfo = response.data;\r\n              // 将用户信息记录cookie\r\n              cookie.set(\"huiju_ucenter\", this.loginInfo, {\r\n                domain: \"localhost\",\r\n              });\r\n            });\r\n          //跳转页面\r\n          this.$router.push({\r\n            name: \"Home\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n    checkPhone() {\r\n      if (!/^1[34578]\\d{9}$/.test(this.user.phone) && this.user.phone != \"\") {\r\n        this.$notify(\"手机号码格式不正确\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.login-container {\r\n  width: 80%;\r\n  height: 100%;\r\n  margin: 0 auto;\r\n}\r\n.input {\r\n  height: 1rem;\r\n  width: 100%;\r\n  font-size: 0.4rem;\r\n  margin: 0.4rem 0;\r\n}\r\n.images {\r\n  margin: 0.2rem 0;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\n</style>"]}]}