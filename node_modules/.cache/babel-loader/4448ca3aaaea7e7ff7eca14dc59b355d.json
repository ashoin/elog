A,IAAA,YAAA,GAAA;AACA,WAAA,OAAA,CACA,OADA,CACA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,OAAA,EAAA;AAFA,OADA,EAKA,IALA,CAKA,MAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA,aAAA,MAAA,CAAA,OAAA,CAAA,MAAA;AACA,OARA,EASA,KATA,CASA,MAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA,OAXA;AAYA,KAlBA;;AAmBA;AACA,IAAA,UAAA,GAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,WADA;AAEA,QAAA,KAAA,EAAA;AAFA,OAAA;AAIA,KAzBA;;AA0BA;AACA,IAAA,eAAA,GAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;;AACA,UAAA,KAAA,OAAA,IAAA,IAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,UAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA;AACA,KAlCA;;AAmCA;AACA,IAAA,aAAA,CAAA,OAAA,EAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,KAtCA;;AAuCA;AACA,IAAA,IAAA,CAAA,IAAA,EAAA;AACA,WAAA,UAAA,CAAA,IAAA;AACA;;AA1CA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"cart-container\">\r\n    <!-- 头部标题 -->\r\n    <div class=\"cart-head\">\r\n      <span>购物车({{ count }})</span>\r\n      <!-- TODO count 为 0 的时候要将（）去除 -->\r\n      <div @click=\"Edit(false)\" v-if=\"storeEdit\">编辑</div>\r\n      <div @click=\"Edit(true)\" v-else>完成</div>\r\n    </div>\r\n    <!-- 主体部分展示 -->\r\n    <div class=\"cart-main\">\r\n      <!-- 如果购物车是空的情况下展示 -->\r\n      <div class=\"cart-empty\" v-if=\"!count\">\r\n        <van-empty\r\n          class=\"custom-image\"\r\n          image=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/shoppingcart/nullcar%402x.png\"\r\n          description=\"购物车还是空的\"\r\n        />\r\n      </div>\r\n      <!-- 如果购物车中有商品的情况下展示 -->\r\n      <div class=\"cart-goods\" v-else>\r\n        <!-- 商品展示组件 -->\r\n        <cart-goods-item\r\n          :message=\"message\"\r\n          @childChange=\"childCheckAll\"\r\n          ref=\"child\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <!-- 底部结算区域 -->\r\n    <div class=\"cart-up-foot\">\r\n      <!-- 结算 -->\r\n      <van-submit-bar\r\n        v-if=\"storeEdit\"\r\n        :price=\"price\"\r\n        button-text=\"去结算\"\r\n        @submit=\"toOrderPay\"\r\n      >\r\n        <!-- 重点的全选功能 -->\r\n        <van-checkbox v-model=\"checked\" @click=\"checkedAllGoods\">\r\n          全选\r\n        </van-checkbox>\r\n      </van-submit-bar>\r\n      <!-- 删除和收藏 -->\r\n      <div class=\"delete-collect\" v-else>\r\n        <van-checkbox v-model=\"checked\" @click=\"checkedAllGoods\">\r\n          全选\r\n        </van-checkbox>\r\n        <van-button plain type=\"default\">收藏</van-button>\r\n        <van-button type=\"danger\" @click=\"handleDelete\">删除</van-button>\r\n      </div>\r\n    </div>\r\n    <!-- 底部导航区 -->\r\n    <div class=\"cart-foot\">\r\n      <wsf-foot :now-active=\"nowActive\" />\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport WsfFoot from \"../components/myModule/WsfFoot\";\r\nimport CartGoodsItem from \"../components/shoppingcart/CartGoodsItem\";\r\nimport { mapState, mapMutations } from \"vuex\";\r\nexport default {\r\n  components: {\r\n    WsfFoot,\r\n    CartGoodsItem,\r\n  },\r\n  data() {\r\n    return {\r\n      change: false,\r\n      nowActive: 3,\r\n      checked: false,\r\n      count: 1,\r\n      message: \"\",\r\n    };\r\n  },\r\n\r\n  created() {\r\n    if (this.$route.query.userMessage != \"\") {\r\n      this.message = this.$route.query.userMessage;\r\n    }\r\n    if (this.goods == \"\") {\r\n      this.count = 0;\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    ...mapState(\"saveOrder\", {\r\n      storeEdit: \"edit\",\r\n      price: \"price\",\r\n      goods: \"goods\",\r\n    }),\r\n  },\r\n\r\n  methods: {\r\n    ...mapMutations(\"saveOrder\", {\r\n      changeEdit: \"changeEdit\",\r\n    }),\r\n    // 删除按键方法\r\n    handleDelete() {\r\n      this.$dialog\r\n        .confirm({\r\n          title: \"确认删除\",\r\n          message: \"木质设计感茶几\",\r\n        })\r\n        .then(() => {\r\n          this.count = 0;\r\n          this.$toast.success(\"删除成功\");\r\n        })\r\n        .catch(() => {\r\n          this.count = 3;\r\n        });\r\n    },\r\n    // 去结算方法\r\n    toOrderPay() {\r\n      this.$router.push({\r\n        name: \"CartOrder\",\r\n        query: \"\",\r\n      });\r\n    },\r\n    // 全选按键功能\r\n    checkedAllGoods() {\r\n      console.log(this.storeEdit);\r\n      if (this.checked == true) {\r\n        this.$refs.child.checkedAll = true;\r\n      } else {\r\n        this.$refs.child.checkedAll = false;\r\n      }\r\n    },\r\n    // 子传父\r\n    childCheckAll(payload) {\r\n      this.checked = payload;\r\n    },\r\n    // 编辑和完成的功能按键\r\n    Edit(bool) {\r\n      this.changeEdit(bool);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped lang='scss'>\r\n.cart-container {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: #ededed;\r\n  // 头部样式\r\n  .cart-head {\r\n    height: 1.2rem;\r\n    width: 100%;\r\n    line-height: 1.2rem;\r\n    position: fixed;\r\n    background-color: white;\r\n    text-align: center;\r\n    z-index: 1;\r\n    // border-bottom: 1px solid #ededed;\r\n    div {\r\n      position: absolute;\r\n      top: 0;\r\n      right: 0.3rem;\r\n      font-size: 0.32rem;\r\n    }\r\n    span {\r\n      font-size: 0.4rem;\r\n    }\r\n  }\r\n  // 中间部分\r\n  .cart-up-foot {\r\n    /deep/.van-submit-bar {\r\n      bottom: 1.02rem;\r\n    }\r\n    // 点击修改和删除的样式\r\n    .delete-collect {\r\n      position: fixed;\r\n      bottom: 1rem;\r\n      left: 0;\r\n      z-index: 1;\r\n      background-color: white;\r\n      width: 96%;\r\n      height: 1.02rem;\r\n      padding-left: 0.3rem;\r\n      display: flex;\r\n      justify-content: flex-start;\r\n      align-items: center;\r\n      .van-checkbox {\r\n        flex-grow: 1;\r\n      }\r\n      .van-button {\r\n        width: 2rem;\r\n      }\r\n    }\r\n  }\r\n  .cart-main {\r\n    padding: 1.2rem 0 2rem 0;\r\n    border-top: 0.2rem solid #ededed;\r\n    .van-empty {\r\n      padding: 30% 0 70% 0;\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}