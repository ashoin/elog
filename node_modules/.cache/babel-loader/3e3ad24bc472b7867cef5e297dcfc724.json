user-address-container\">\r\n    <!-- 头部标题+添加按钮 -->\r\n    <div class=\"address-head\">\r\n      <div class=\"info-head-title\">\r\n        <van-image\r\n          width=\"0.2rem\"\r\n          height=\"0.5rem\"\r\n          src=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/seckill/back%402x.png\"\r\n          @click=\"toMineUserInfo\"\r\n        />\r\n        <span>地址</span>\r\n        <van-image\r\n          width=\"0.5rem\"\r\n          height=\"0.5rem\"\r\n          src=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/mine/%E4%B8%AA%E4%BA%BA%E4%BF%A1%E6%81%AF/add%402x.png\"\r\n          @click=\"toAddAddress\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <div class=\"address-main\">\r\n      <!-- 当地址信息为空的时候 -->\r\n      <van-empty\r\n        v-if=\"hasAddress\"\r\n        class=\"custom-image\"\r\n        image=\"https://tiechuimeimeia.oss-cn-hangzhou.aliyuncs.com/mine/%E4%B8%AA%E4%BA%BA%E4%BF%A1%E6%81%AF/nulllocate%402x.png\"\r\n      >\r\n        <div class=\"description\" slot=\"description\">\r\n          <span>您还未添加常用地址</span>\r\n          <span>点击右上角“+” 按钮，添加常用地址</span>\r\n        </div>\r\n      </van-empty>\r\n      <!-- 当存在地址的时候 -->\r\n      <!-- <div class=\"address-main-info\" v-else> -->\r\n      <van-radio-group v-model=\"radio\" checked-color=\"red\">\r\n        <div class=\"info-card\" v-for=\"address in addressList\" :key=\"address.id\">\r\n          <div>\r\n            {{ address.name\r\n            }}<span style=\"margin-left: 0.4rem\">{{ address.tel }}</span>\r\n          </div>\r\n          <span style=\"margin: 0.2rem 0\">\r\n            详细地址：{{ address.periouce + address.street }}\r\n          </span>\r\n          <div class=\"btns\">\r\n            <van-radio :name=\"address.id\">设为默认</van-radio>\r\n            <div>\r\n              <van-button>编辑</van-button>\r\n              <van-button>删除</van-button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </van-radio-group>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      hasAddress: false,\r\n\r\n      radio: 10011,\r\n      addressList: [\r\n        {\r\n          id: 10011,\r\n          name: \"张三\",\r\n          tel: \"18888888888\",\r\n          periouce: \"浙江省杭州市\",\r\n          street: \"街道办9号\",\r\n        },\r\n        {\r\n          id: 10012,\r\n          name: \"李四\",\r\n          tel: \"18888888888\",\r\n          periouce: \"浙江省杭州市亏\",\r\n          street: \"街道办9号\",\r\n        },\r\n        {\r\n          id: 10013,\r\n          name: \"铁锤妹妹\",\r\n          tel: \"18888888888\",\r\n          periouce: \"浙江省杭州市江干区反贪局\",\r\n          street: \"街道办9号\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    toMineUserInfo() {\r\n      this.$router.push({\r\n        name: \"MineUserInfo\",\r\n      });\r\n    },\r\n    toAddAddress() {\r\n      this.$router.push({\r\n        name: \"AddressAdd\",\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.user-address-container {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: #ededed;\r\n  .address-head {\r\n    padding-bottom: 1.4rem;\r\n  }\r\n  .info-head-title {\r\n    width: 95%;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    z-index: 1;\r\n    height: 1.2rem;\r\n    background-color: white;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    padding: 0 0.2rem;\r\n    border-bottom: 1px solid #ededed;\r\n    span {\r\n      font-size: 0.4rem;\r\n      text-align: center;\r\n      flex-grow: 1;\r\n    }\r\n  }\r\n  .address-main {\r\n    width: 100%;\r\n\r\n    // 没有地址的情况\r\n    .van-empty {\r\n      padding: 30% 0 70% 0;\r\n      width: 100%;\r\n      /deep/.van-empty__image {\r\n        width: 3rem;\r\n        height: 3.5rem;\r\n      }\r\n      /deep/.van-empty__description {\r\n        padding: 0;\r\n      }\r\n      .description {\r\n        width: 100%;\r\n        display: flex;\